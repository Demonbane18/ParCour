{
  "files.exclude": {
    "amplify/.config": true,
    "amplify/**/*-parameters.json": true,
    "amplify/**/amplify.state": true,
    "amplify/**/transform.conf.json": true,
    "amplify/#current-cloud-backend": true,
    "amplify/backend/amplify-meta.json": true,
    "amplify/backend/awscloudformation": true
  },
  "cSpell.words": [
    "Dropoff",
    "codersgyan",
    "cour",
    "dbstore",
    "image",
    "msapplication",
    "msapplication tile image",
    "mstile",
    "noty",
    "par",
    "par cour",
    "parcour",
    "precomposed",
    "resave",
    "tile"
  ],
  "textmarker.savedHighlights": [
    {
      "pattern": {
        "type": "string",
        "expression": "[+]",
        "ignoreCase": false,
        "wholeMatch": false
      },
      "color": "lime"
    },
    {
      "pattern": {
        "type": "string",
        "expression": "[-]",
        "ignoreCase": false,
        "wholeMatch": false
      },
      "color": "magenta"
    }
  ]
}