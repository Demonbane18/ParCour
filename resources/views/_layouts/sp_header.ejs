                <li class="ml-6">

                </li>
            </ul>
            <script>
                /* Set the width of the sidebar to 250px and the left margin of the page content to 250px */
                function openNav() {
                    document.getElementById("mySidebar").style.width = "250px";
                    document.getElementById("move-content").style.marginLeft = "250px";
                    document.getElementById("openbtn").setAttribute("onclick", "closeNav()")

                }

                /* Set the width of the sidebar to 0 and the left margin of the page content to 0 */
                function closeNav() {
                    document.getElementById("mySidebar").style.width = "0";
                    document.getElementById("move-content").style.marginLeft = "0";
                    document.getElementById("openbtn").setAttribute("onclick", "openNav()")
                }
            </script>
