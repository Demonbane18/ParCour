    "dotenv": "^8.2.0",
    "ejs": "^3.1.5",
    "express": "^4.17.1",
    "express-ejs-layouts": "^2.5.0",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.1",
    "http-errors": "^1.8.0",
    "moment": "^2.28.0",
    "mongoose": "^5.10.5",
    "noty": "^3.2.0-beta",
    "order-id": "^2.0.0",
    "passport": "^0.4.1",
